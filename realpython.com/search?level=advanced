      <div v-infinite-scroll="loadMore" infinite-scroll-disabled="loadMoreDisabled" infinite-scroll-distance="10">
        <div v-for="(result, index) in results" v-bind:key="result.key">
          

<div class="container my-3">
  <div class="row">

    <div class="col-12 col-sm-4 align-self-top px-0" aria-hidden="true" aria>
